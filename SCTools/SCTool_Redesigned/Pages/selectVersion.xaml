<Page x:Class="SCTool_Redesigned.Pages.selectVersion"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:SCTool_Redesigned.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="440" d:DesignWidth="800"
      Title="selectVersion">
    <Grid>
        <Label x:Name="PhaseNumber" Content="02" FontWeight="Bold" HorizontalAlignment="Left" Margin="50,40,0,0" VerticalAlignment="Top" Foreground="{StaticResource TextBrush}" FontSize="72"/>
        <Label x:Name="Phasetext" Content="버전을 선택하고 설치합니다." HorizontalAlignment="Left" Margin="50,130,0,0" VerticalAlignment="Top" Foreground="{StaticResource TextBrush}" FontSize="24"/>
        <Button x:Name="PhaseBtn" Content="버전 불러오기" HorizontalAlignment="Right" Height="40" Margin="0,0,50,30" VerticalAlignment="Bottom" Width="140" Style="{StaticResource subBtnStyle}"/>
    </Grid>
</Page>
